generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model VideoAccess {
  id           Int      @id @default(autoincrement())
  fio_or_name  String
  video_title  String
  access_status Int

  video        Videos   @relation(fields: [video_title], references: [video_title])
}

model Videos {
  id          Int      @id @default(autoincrement())
  link        String   @unique
  video_title String   @unique
  created_at  DateTime @default(now())
  start_date  DateTime
  end_date    DateTime

  accesses    VideoAccess[]
}

